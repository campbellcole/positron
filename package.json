{
  "name": "positron",
  "version": "2.0.0-dev",
  "author": "Campbell M. Cole",
  "main": "./public/electron.js",
  "description": "homework manager by campbell cole",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "bootstrap": "^5.0.1",
    "moment": "^2.29.1",
    "node-fetch": "^2.6.1",
    "normalize-url": "^6.0.1",
    "react": "^17.0.2",
    "react-big-calendar": "^0.33.5",
    "react-custom-scroll": "^4.3.0",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-router-dom": "^5.2.0",
    "react-select": "^4.3.1",
    "sass": "^1.34.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "electron": "^13.1.2",
    "electron-builder": "^22.11.7",
    "electron-is-dev": "^2.0.0",
    "eslint-plugin-react": "^7.24.0",
    "react-scripts": "^4.0.3",
    "wait-on": "^5.3.0"
  },
  "scripts": {
    "start": "concurrently \"npm run serve\" \"wait-on http://localhost:3000 && electron .\"",
    "serve": "react-scripts start",
    "build": "react-scripts build",
    "preelectron-pack": "npm run build",
    "electron-pack": "electron-builder",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "build": {
    "appId": "com.campbellcole.positron",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "browserslist": {
    "production": [
      "last 1 electron version"
    ],
    "development": [
      "last 1 electron version"
    ]
  }
}
