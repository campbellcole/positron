{
  "parserOptions": {
      "ecmaVersion": 11,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "comma-dangle" : 1,
    "semi": [1, "never"]
  },
  "settings": {
    "react": {
      "pragma": "React",
      "version": "16.13.1"
    }
  }
}